<h1>Editing subscription_fu_log</h1>

<% form_for(@subscription_fu_log) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.label :note %><br />
    <%= f.text_field :note %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @subscription_fu_log %> |
<%= link_to 'Back', subscription_fu_logs_path %>